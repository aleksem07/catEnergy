@media (min-width: $tablet-width) {
  .main-nav {
    display: inline-block;
    width: 440px;
  }
}

.main-nav__list {
  position: relative;
  display: flex;
  // flex-wrap: wrap;
  flex-direction: column;
  align-content: center;
  width: 100%;
  margin: 0;
  padding: 0;
  color: $basic_color;
  text-align: center;
  text-transform: uppercase;
  list-style: none;

  @media (min-width: $tablet-width) {
    flex-direction: row;
    width: 100%;
    height: 24px;
  }
}

@media (min-width: $tablet-width) {
  .site-list {
    font-size: 18px;
    line-height: 24px;
  }
}

.site-list__item {
  width: 320px;
  padding: 22px 0;
  border-bottom: 1px solid $grey_line;
  font-weight: 500;

  &:first-child {
  border-top: 1px solid $grey_line;
  }

  @media (min-width: $tablet-width) {
    width: max-content;
