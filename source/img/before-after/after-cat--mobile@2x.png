            <fieldset class="form-section form-section--comment">
              <legend class="form__legend">
                <span>Комментарий</span>
              </legend>
              <textarea
                name="comment"
                cols="35"
                rows="10"
                placeholder="Расскажите обо всех повадках кота"
              ></textarea>
            </fieldset>

            <fieldset class="form-section form-section--additionally">
              <legend class="form__legend">
                <span>Дополнительно</span>
              </legend>

              <label class="form__item">
                <input
                  class="form__input visually-hidden"
                  type="checkbox"
                  name="add"
                  value="sweetener"
                  checked
                />
                <span class="form__label additionally">сахарозаменитель </span>
              </label>

              <label class="form__item">
                <input
                  class="form__input visually-hidden"
                  type="checkbox"
                  name="add"
                  value="water"
                />
                <span class="form__label additionally">питьевая вода</span>
              </label>

              <label class="form__item">
                <input
                  class="form__input visually-hidden"
                  type="checkbox"
                  name="add"
                  value="milk"
                />
                <span class="form__label additionally">молоко</span>
              </label>

              <label class="form__item">
                <input
                  class="form__input visually-hidden"
                  type="checkbox"
                  name="add"
                  value="vitamins"
                />
                <span class="form__label additionally">витамины</span>
              </label>
              <button class="form__button button button--green" type="submit">
                отправить заявку
              </button>
              <p class="program-select__need">* — Обязательные поля</p>
            </fieldset>
          </form>
        </div>
