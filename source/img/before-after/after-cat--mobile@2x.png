}

.form__legend span {
  display: inline-block;
  padding-right: 14px;
  background-color: #FFFFFF;
}

.form-section--comment {
  width: 100%;
  margin-bottom: 22px;
}

.form-section--comment > textarea {
  resize: none;
  border: 1px solid #E7E7E7;
}

.form-section--comment > textarea::-moz-placeholder {
  padding-top: 9px;
  text-align: center;
  text-transform: uppercase;
  font-family: "Oswald";
  font-size: 16px;
  line-height: 18px;
}

.form-section--comment > textarea::placeholder {
  padding-top: 9px;
  text-align: center;
  text-transform: uppercase;
  font-family: "Oswald";
  font-size: 16px;
  line-height: 18px;
}

.additionally {
  width: auto;
  height: 30px;
  margin-left: 40px;
}

.form__input + .additionally::before {
  content: "";
  width: 30px;
  height: 30px;
  position: absolute;
  border: 1px solid #E7E7E7;
  top: -8px;
  left: 0;
}

.form__input:checked + .additionally::after {
  content: "";
  width: 30px;
  height: 30px;
  position: absolute;
  background-image: url(../img/icon/check.svg);
  background-repeat: no-repeat;
  top: 0;
  left: 7px;
}

.form__input:focus + .additionally::before {
  outline: 2px solid #222222;
}

