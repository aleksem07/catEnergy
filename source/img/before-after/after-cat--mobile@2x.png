.form-section {
  margin-bottom: 26px;
}

.form-section--program {
  border-top: 1px solid #D9D9D9;
  border-bottom: 1px solid #D9D9D9;
}

.form__item {
  position: relative;
  display: flex;
  align-items: center;
  margin-bottom: 13px;
}

.form__item:last-child {
  margin-bottom: 0;
}

.form__label {
  flex-shrink: 0;
  margin-right: 15px;
  text-transform: uppercase;
  font-size: 16px;
  line-height: 18px;
}

.form__input {
  padding: 13px 0 15px 14px;
  border: 1px solid #E7E7E7;
}
.form__input::-moz-placeholder {
  text-transform: uppercase;
  color: #444444;
}
.form__input::placeholder {
  text-transform: uppercase;
  color: #444444;
}

.form-section--program {
  padding-bottom: 19px;
}

.radio__wrapper {
  padding-top: 20px;
}

.form__radio {
  margin-left: 40px;
  padding-bottom: 13px;
}

.form__item:last-child .form__radio {
  padding: 0;
}

.form__input + .form__radio::before {
  content: "";
  width: 30px;
  height: 30px;
  position: absolute;
  border: 1px solid #CDCDCD;
  border-radius: 50%;
  top: -5px;
  left: 0;
}

.form__input:checked + .form__radio:after {
  content: "";
  width: 14px;
  height: 14px;
  position: absolute;
  background-color: #68B738;
  border-radius: 50%;
  top: 10%;
  left: 8px;
}

.form__input:focus + .form__radio::before {
  outline: 2px solid #222222;
}

.form__legend {
  width: 100%;
  margin-bottom: 35px;
