    <main class="page-main">
      <section class="program-select">
        <div class="program-select__wrapper">
          <h1 class="program-select__title">Подбор программы</h1>
          <p class="program-select__text">
            Заполните анкету, и мы подберем<br />
            программу питания для вашего кота
          </p>

          <form class="form" action="https://echo.htmlacademy.ru" method="post">
            <div class="form-section--desktop__wrapper">
              <fieldset class="form-section form-section--cat">
                <legend class="visually-hidden">Информация о коте</legend>
                <label class="form__item">
                  <span class="form__label">Имя:*</span>
                  <input
                    class="form__input"
                    type="text"
                    name="cat-name"
                    placeholder="Барсик"
                    required
                    autofocus
                  />
                </label>
                <label class="form__item">
                  <span class="form__label">Вес (кг):*</span>
                  <input
                    class="form__input"
                    type="text"
                    name="cat-weight"
                    placeholder="7"
                    pattern="^[ 0-9]+$"
                    required
                  />
                </label>
                <label class="form__item">
                  <span class="form__label">Возраст (лет):</span>
                  <input
                    class="form__input"
                    type="text"
                    name="cat-age"
                    placeholder="7"
                  />
                </label>
              </fieldset>
              <fieldset class="form-section form-section--program">
                <legend class="visually-hidden">Программа</legend>
                <div class="radio__wrapper">
                  <label class="form__item">
                    <input
                      class="form__input visually-hidden"
                      type="radio"
                      name="program"
                      value="slim"
                      checked
                    />
                    <span class="form__label form__radio">Похудение</span>
                  </label>
                  <label class="form__item">
                    <input
                      class="form__input visually-hidden"
                      type="radio"
                      name="program"
                      value="pro"
                    />
                    <span class="form__label form__radio">Набор массы</span>
                  </label>
                  <label class="form__item">
                    <input
                      class="form__input visually-hidden"
                      type="radio"
                      name="program"
                      value="need-help"
                    />
                    <span class="form__label form__radio"
                      >Не знаю (Нужен ваш совет)</span
                    >
                  </label>
                </div>
              </fieldset>
            </div>

            <fieldset class="form-section form-section--owner">
              <legend class="form__legend">
                <span class="form__legend--title"
                  >Контактные данные <br />
                  (владельца кота)
                </span>
              </legend>

              <label class="form__item">
                <span class="form__label">E-mail:*</span>
                <input
                  class="form__input"
                  type="email"
                  name="contact-email"
                  placeholder="kuklachev@gmail.com"
                  required
                />
                <svg
                  class="form__input--email"
                  width="19"
                  height="14"
                  viewBox="0 0 19 14"
                  fill="none"
