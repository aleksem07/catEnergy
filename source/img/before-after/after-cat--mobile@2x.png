img {
  max-width: 100%;
  height: auto;
}

a {
  color: #000000;
  text-decoration: none;
}

a:visited {
  color: #000000;
}

.visually-hidden {
  position: absolute;
  width: 1px;
  height: 1px;
  margin: -1px;
  border: 0;
  padding: 0;
  white-space: nowrap;
  -webkit-clip-path: inset(100%);
          clip-path: inset(100%);
  clip: rect(0 0 0 0);
  overflow: hidden;
}

*,
*::before,
*::after {
  box-sizing: border-box;
}

fieldset {
  border: 0;
  margin: 0;
  padding: 0;
}

label {
  cursor: pointer;
}

input:focus {
  border: 2 px solid #444444;
}

.button {
  padding: 7px 44px;
  cursor: pointer;
  border: 0;
  background-color: #68B738;
  font-size: 14px;
  line-height: 18px;
  text-transform: uppercase;
}

a.button--intro {
  display: block;
  width: 100%;
  color: #FFFFFF;
}

.main-nav__toogle {
  background: transparent;
  border: 0;
  cursor: pointer;
}

.form__button {
  width: 100%;
  color: #FFFFFF;
}

.program-select__need {
  text-align: center;
  margin-bottom: 32px;
}

a.button--product {
  width: 280px;
  padding-top: 10px;
  grid-column: 1/-1;
  grid-row: 3/4;
  color: #FFFFFF;
  text-align: center;
  font-size: 16px;
  line-height: 20px;
}

a.button--view-all {
  background-color: #F2F2F2;
  width: 280px;
  padding-top: 10px;
  font-size: 16px;
  color: #444444;
}

.page {
  height: 100%;
  font-family: Oswald;
  font-size: 20px;
  font-weight: 400;
  line-height: 20px;
}

.page__body {
  display: flex;
  flex-direction: column;
  width: 320px;
  min-height: 100%;
  margin: 0 auto;
  color: #000000;
  padding: 0 20px;
}
@media (min-width: 768px) {
  .page__body {
    width: 768px;
