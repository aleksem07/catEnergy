ly-hidden {\n  position: absolute;\n  width: 1px;\n  height: 1px;\n  margin: -1px;\n  border: 0;\n  padding: 0;\n  white-space: nowrap;\n  clip-path: inset(100%);\n  clip: rect(0 0 0 0);\n  overflow: hidden;\n}\n\n*,\n*::before,\n*::after {\n  box-sizing: border-box;\n}\n\nfieldset {\n  border: 0;\n  margin: 0;\n  padding: 0;\n}\n\nlabel {\n  cursor: pointer;\n}\n\ninput:focus {\n  border: 2 px solid #444444;\n}\n\n.button {\n  padding: 7px 44px;\n  cursor: pointer;\n  border: 0;\n  background-color: #68B738;\n  font-size: 14px;\n  line-height: 18px;\n  text-transform: uppercase;\n}\n\na.button--intro {\n  display: block;\n  width: 100%;\n  color: #FFFFFF;\n}\n\n.main-nav__toogle {\n  background: transparent;\n  border: 0;\n  cursor: pointer;\n}\n\n.form__button {\n  width: 100%;\n  color: #FFFFFF;\n}\n\n.program-select__need {\n  text-align: center;\n  margin-bottom: 32px;\n}\n\na.button--product {\n  width: 280px;\n  padding-top: 10px;\n  grid-column: 1/-1;\n  grid-row: 3/4;\n  color: #FFFFFF;\n  text-align: center;\n  font-size: 16px;\n  line-height: 20px;\n}\n\na.button--view-all {\n  background-color: #F2F2F2;\n  width: 280px;\n  padding-top: 10px;\n  font-size: 16px;\n  color: #444444;\n}\n\n.page {\n  height: 100%;\n  font-family: Oswald;\n  font-size: 20px;\n  font-weight: 400;\n  line-height: 20px;\n}\n\n.page__body {\n  display: flex;\n  flex-direction: column;\n  width: 320px;\n  min-height: 100%;\n  margin: 0 auto;\n  color: #000000;\n  padding: 0 20px;\n}\n@media (min-width: 768px) {\n  .page__body {\n    width: 768px;\n    outline: 5px solid rgba(31, 12, 112, 0.589);\n    outline-offset: -5px;\n  }\n}\n@media (min-width: 1440px) {\n  .page__body {\n    width: 1440px;\n    outline: 5px solid rgba(247, 9, 9, 0.589);\n    outline-offset: -5px;\n  }\n}\n\n.logo__wrapper {\n  display: flex;\n  justify-content: space-between;\n  align-items: center;\n  margin-top: 12px;\n  margin-bottom: 22px;\n}\n@media (min-width: 768px) {\n  .logo__wrapper {\n    width: max-content;\n    gap: 20px;\n    display: inline-flex;\n  }\n}\n\n@media (min-width: 768px) {\n  .main-nav__toogle {\n    display: none;\n  }\n}\n.current {\n  border-bottom: 2px solid #68B738;\n}\n\n@media (min-width: 768px) {\n  .main-nav {\n    display: inline-block;\n    width: 440px;\n  }\n}\n.main-nav__list {\n  position: relative;\n  display: flex;\n  flex-direction: column;\n  align-content: center;\n  width: 100%;\n  margin: 0;\n  padding: 0;\n  color: #000000;\n  text-align: center;\n  text-transform: uppercase;\n  list-style: none;\n}\n@media (min-width: 768px) {\n  .main-nav__list {\n    flex-direction: row;\n    width: 100%;\n  }\n}\n\n@media (min-width: 768px) {\n  .site-list {\n    font-family: \"Oswald\";\n    width: max-content;\n    font-size: 18px;\n    line-height: 24px;\n  }\n}\n.site-list__item {\n  width: 320px;\n  padding: 22px 0;\n  border-bottom: 1px solid #E6E6E6;\n  font-weight: 500;\n}\n.site-list__item:first-child {\n  border-top: 1px solid #E6E6E6;\n}\n\n.main-intro {\n  text-align: center;\n  margin-bottom: 22px;\n}\n@media (min-width: 768px) {\n  .main-intro {\n    display: block;\n  }\n}\n\n.main-intro .main-intro--bg {\n  width: 320px;\n  margin-left: -20px;\n  background-image: linear-gradient(rgba(104, 183, 56, 0.85), rgba(104, 183, 56, 0.85)), url(../img/bg/cat.jpg);\n  background-size: 320px 302px;\n  background-repeat: no-repeat;\n  background-color: #fff;\n}\n\n.main-intro__title {\n  color: #FFFFFF;\n  padding-top: 27px;\n  margin: 0;\n  margin-bottom: 25px;\n  font-weight: 400;\n  font-size: 36px;\n  line-height: 36px;\n}\n\n.main-intro__text {\n  color: #FFFFFF;\n  margin-bottom: 29px;\n  font-style: normal;\n  font-weight: 400;\n  font-size: 14px;\n  line-height: 14px;\n  text-transform: uppercase;\n}\n\n.catalog {\n  position: relative;\n  padding: 0 20px 18px 20px;\n  background-color: #F2F2F2;\n}\n\n.catalog__title {\n  padding-top: 30px;\n  font-size: 24px;\n  line-height: 37px;\n  font-weight: 400;\n  text-transform: