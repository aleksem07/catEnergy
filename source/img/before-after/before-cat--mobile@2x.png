@media (min-width: $tablet-width) {
  .site-list {
    font-family: 'Oswald';
width: max-content;
    font-size: 18px;
    line-height: 24px;
  }
}

.site-list__item {
  width: 320px;
  padding: 22px 0;
  border-bottom: 1px solid $grey_line;
  font-weight: 500;

  &:first-child {
  border-top: 1px solid $grey_line;
