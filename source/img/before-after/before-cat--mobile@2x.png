@media (min-width: $tablet-width) {
  .site-list {
    font-family: 'Oswald';
width: 200px;
    font-size: 18px;
    line-height: 24px;
  }
}

.site-list__item {
  width: 320px;
  padding: 22px 0;
  border-bottom: 1px solid $grey_line;
  font-weight: 500;

